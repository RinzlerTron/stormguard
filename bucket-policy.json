{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadAccess",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::stormguard-deploy-bucket",
        "arn:aws:s3:::stormguard-deploy-bucket/*"
      ]
    }
  ]
}
